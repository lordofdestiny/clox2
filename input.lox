var zero;
var one;
var two;
var three;
var four;


for(var i = 0; i < 5; i=i+1) {
    fun capture() {
        print i;
    }
    switch(i) {
        case 0:
            zero = capture;
            break;
        case 1:
            one = capture;
            break;
        case 2:
            two = capture;
            break;
        case 3:
            three = capture;
            break;
        case 4:
            four = capture;
            break;
    }
    print i;
}