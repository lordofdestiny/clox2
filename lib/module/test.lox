fun timeme(function) {
    var start = clock();
    function();
    var end = clock();

    print "Elapsed: " + (end - start);
}

print "Some long work:";

var N = 10000000;

timeme(|| {
    var pp = 0;
    for(var i = 0; i < N; i+=1) {
        pp += i;
    }
});

fun my_max(a, b) {
    return a < b ? b : a;
}

timeme(|| {
    for (var i = 0; i < N; i += 1) max(55, 99);
});

timeme(|| {
    for (var i = 0; i < N; i += 1) my_max(55, 99);
});

